{
  "name": "plugin",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kubera-apps.github.io/plugin",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
